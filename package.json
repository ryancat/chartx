{
  "name": "chartx",
  "version": "0.0.1",
  "description": "High performance and large scale charting library",
  "main": "dist/chartx.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel build:watch test open",
    "poststart": "babel-node tools/endMessage.js",
    "build": "webpack --config webpack.config.js",
    "build:watch": "npm run build -- --watch",
    "build:prod": "webpack --config webpack.prod.config.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "open": "open demo.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryancat/chartx.git"
  },
  "author": "Ryan Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryancat/chartx/issues"
  },
  "homepage": "https://github.com/ryancat/chartx#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.2",
    "colors": "^1.1.2",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "expect": "^21.0.2",
    "immer": "^1.3.0",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.1",
    "open": "0.0.5",
    "path": "^0.12.7",
    "webpack": "^3.11.0"
  },
  "keywords": [
    "chart"
  ],
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "regenerator-runtime": "^0.11.1",
    "uuid": "^3.1.0"
  }
}
